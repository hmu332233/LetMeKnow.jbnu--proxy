    "@shelf/jest-mongodb": "^1.1.3",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/supertest": "^2.0.10",
    "husky": "^4.3.7",
    "jest": "^24.9.0",
    "lint-staged": "^10.5.3",
    "morgan": "^1.9.1",
    "prettier": "^2.2.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
